{
  "routes": [
    {
      "route": "/:page",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "rewrite": "/server/server.js"
    },
    {
      "route": "/api/get-sas-url",
      "methods": ["POST"],
      "rewrite": "/server/server.js"
    },
    {
      "route": "/server/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "rewrite": "/server/server.js"
    }
  ],
  "navigationFallback": {
    "rewrite": "/client/index.html"
  },
  "platform": "node",
  "apiLocation": "/server"
}