{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/",
        "rewrite": "../client/home.html",
        "statusCode": 200
      },
      {
        "route": "/*",
        "rewrite": "../client/404.html",
        "statusCode": 404
      }
    ]
  }